%h1 Sign Up
= form_for User.new, class: "form-inline" do |f|
  = f.label :email
  = f.text_field :email
  = f.label :password
  = f.password_field :password
  = f.label :password_confirmation
  = f.password_field :password_confirmation
  %br/
  = f.label :avatar, 'Upload Avatar'
  = f.file_field :avatar
  %br/
  = f.submit "Sign Up"
